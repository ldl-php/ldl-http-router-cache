{
  "routes": [
    {
      "version": "1.0",
      "url": {
        "prefix": "list/{urlName}"
      },
      "request": {
        "method": "get"
      },
      "name": "test",
      "description": "A test route",
      "dispatcher": "test.cache.dispatcher",
      "middleware": {
        "preDispatch": "cachePreDispatch",
        "dispatchers": "cacheDispatcher",
        "postDispatch": "cachePostDispatch"
      },
      "cache": {
        "active": true,
        "priority": 1,
        "adapter": "fs.adapter",
        "config": {
          "enabled": true,
          "purgeable": false,
          "expiresAt": "+1 minutes"
        }
      }
    }
  ]
}
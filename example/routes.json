{
  "routes": [
    {
      "name": "test",
      "version": "1.0",
      "description": "A test route",
      "url": {
        "prefix": "list/{urlName}"
      },
      "request": {
        "method": "get"
      },
      "middleware": {
        "preDispatch": [
          "cache.preDispatch"
        ],
        "dispatchers": [
          "test.cache.dispatcher"
        ],
        "postDispatch": [
          "cache.postDispatch"
        ]
      },
      "cache": {
        "adapter": "fs.adapter",
        "config": {
          "enabled": true,
          "purgeable": false,
          "expiresAt": "+1 minutes"
        }
      }
    }
  ]
}
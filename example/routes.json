{
  "routes": [
    {
      "name": "test",
      "version": "1.0",
      "description": "A test route",
      "url": {
        "prefix": "list/{urlName}"
      },
      "request": {
        "method": "get"
      },
      "middleware": {
        "preDispatch": {
          "list":[
            "cache.predispatch"
          ]
        },
        "dispatchers": {
          "list": [
            "test.cache.dispatcher"
          ]
        },
        "postDispatch": {
          "list":[
            "cache.postdispatch"
          ]
        }
      },
      "cache": {
        "adapter": "fs.adapter",
        "config": {
          "enabled": true,
          "purgeable": false,
          "expiresAt": "+1 minutes",
          "keyGenerator": "request.parameter.values",
          "keyGeneratorOptions": {
            "debug": true,
            "get": {
              "list": "all",
              "casing":{
                "key": "lower"
              }
            }
          }
        }
      }
    }
  ]
}